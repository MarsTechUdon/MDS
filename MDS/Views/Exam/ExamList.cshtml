<script>
    $("#load").click(function () {
        var count = document.getElementById("countexamnotok").value;
        var count_num = parseInt(count)
        var $btn = $(this);
        $btn.button('loading');
        // simulating a timeout
        if (count_num < 50) {
            setTimeout(function () {
                $btn.button('reset');
            }, 1000);
        }
        else {
            setTimeout(function () {
                $btn.button('reset');
            }, 2000000);
        }

    });

</script>

<script>
    function fnc_send() {
        ///ส่งไปตอบ///
        var examid = $('#examid').val();
        var examqno = $('#examqno').val();
        var examanswerno = $('#examanswerno').val();

        $.ajax({
            type: "POST",
            url: "AddExam/Exam",
            //  dataType: "html",
            data: JSON.stringify({ examid: examid, examQNO: examqno, examAnswerNo: examanswerno }),
            contentType: "application/json",
            success: function (result) {
                $('#vama').append("");
                // console.log(result);
            },
            error: function (request, status, error) {
                //Do Something on Failure
            }
        });
        ///-------////

        var count_choice_radio = $(':radio[class="count_choice_radio"]:checked').length;
        var count = parseInt(count_choice_radio)
        // alert(count);
        if (count < 50) {
            $('#countexamnot').modal('show');
            document.getElementById("countexamnotok").value = count;
        }
        else {
            document.getElementById("countexamnotok").value = count;
            document.getElementsByClassName("chk_send")[0].click();
        }
    }
</script>

<script>
    ///เก็ตเสียงคำตอบ เป็นรายข้อ///
    var p = 0;
    function chk_a_sound(temp_examid, examQNO, a_examanswerno) {
        var temp_no = parseInt(a_examanswerno)
        $.ajax({
            type: "POST",
            url: "GetASound/Exam",
            data: JSON.stringify({ examid: temp_examid, examQNO: examQNO, examAnswerNo: a_examanswerno }),
            contentType: "application/json",
            success: function (result) {

                var a_sound = "data:audio/mp3;base64," + result.ChoiceSound;
                var snd = new Audio(a_sound);
                snd.play();
            },
            error: function (request, status, error) {
                //Do Something on Failure
