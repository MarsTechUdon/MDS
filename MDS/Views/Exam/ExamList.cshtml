<div class="modal fade bs-example-modal-sm" id="countexam" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-sm">
        <div class="modal-content">
            <div class="modal-header bg-orange">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>

                @if (@ViewBag.LangValue == "TH")
                {<h4 class="modal-title">แบบทดสอบ</h4>}
                else
                { <h4 class="modal-title">Pretest</h4>}
            </div>
            <div class="modal-body">

                @if (@ViewBag.LangValue == "TH")
                {<h3>ข้อสอบที่ทำไป <b id="count_exam_show"></b> / 50 ข้อ</h3>}
                else
                { <h3>Do it <b id="count_exam_show"></b> / 50 choice</h3>}

                <input type="hidden" id="count_exam" value="" style="width:25px;" disabled />
                @if (@ViewBag.LangValue == "TH")
                {<h4><b id="count_exam_not_ok" style="color:orange;">ทำข้อสอบให้ครบ 50 ข้อ</b></h4>}
                else
                { <h4 class="modal-title"></h4>}


            </div>
            <div class="modal-footer">

                @if (@ViewBag.LangValue == "TH")
                {<button type="button" id="btn_back" class="btn btn-warning" data-dismiss="modal">กลับไปทำข้อสอบต่อ</button>}
                else
                { <button type="button" id="btn_back" class="btn btn-warning" data-dismiss="modal">Back to Exam</button>}

                @using (Html.BeginForm("ExamListScore", "Exam", FormMethod.Post))
                {
                    <input type="hidden" id="" name="studentind" value="@ViewBag.temp_studentind">
                    <input type="hidden" id="" name="examid" value="@ViewBag.temp_examid">
                    <input type="hidden" id="" name="examid" value="@ViewBag.temp_studentind">
                    <input type="hidden" id="" name="LangValue" value="@ViewBag.LangValue">
                    // <input type="hidden" id="score_exam" name="score" value="">
                    <button type="submit" id="count_exam_ok_na" class="btn btn-success chk_send" data-loading-text="กรุณารอ..." style="display:none;">ยืนยันการส่งข้อสอบ</button>
                    <h4><b id="count_exam_ok" style="color:orange;">กดปุ่ม "สรุป/ส่งข้อสอบ"</b></h4>
                }
            </div>
        </div>
    </div>
</div>
