            var couponNo = $("#discount").val();
            $.ajax({
                 url: '@Url.Action("VerifyCoupon", "Payment")',
                type: 'POST',
                contentType: 'application/json',
                dataType: 'Json',
                data: JSON.stringify({ couponno: couponNo }),
                success: successFunction,
                error: errorFunction
            });
            function successFunction(result) {
                if (result.success == 0) {
                    $('#msg_code').css({ "color": "green" });
                    $('#msg_code').addClass('block');
                    $('#msg_code').closest('.form-group').removeClass('has-error');
                    $('#msg_code').closest('.form-group').addClass('has-success');
                    result.amount = checkAmount(result.amount)
                    $('#msg_code').text(result.msg);
                    if (result.amount != '') {
                        $('#btnSubmitPayment').removeAttr("disabled");
                        $('#amountDiscount').val(result.amount);
                        $('#reference').val($('#discount').val());
                    }

                    //$('#msg_code').closest('.form-group').removeClass('has-error');
                    //$('#msg_code').closest('.form-group').addClass('has-success');


                } else if (result.success == 1) {
                    $('#msg_code').text(result.msg);
                    $('#msg_code').css({ "color": "red" });
                    $('#msg_code').addClass('block');
                    $('#msg_code').closest('.form-group').removeClass('has-success');
                    $('#msg_code').closest('.form-group').addClass('has-error');
                    $('#btnSubmitPayment').attr("disabled", "disabled");
                    $('#discount').val('');
                    //alert(result.msg);
                } else {
                    alert('ตรวจสอบส่วนลดมีปัญหา');
                }
            }
            function errorFunction(result) {
                alert('error');
            }
        });

